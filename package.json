{
  "name": "pip-webui2-themes",
  "version": "1.1.24",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build pip-webui2-themes && npm run build:lib-styles",
    "build:lib-styles": "scss-bundle -c ./projects/pip-webui2-themes/scss-bundle.styles.json && scss-bundle -c ./projects/pip-webui2-themes/scss-bundle.variables.json",
    "test": "ng test",
    "test:lib": "ng test pip-webui2-themes",
    "lint": "ng lint",
    "lint:lib": "ng lint pip-webui2-themes",
    "e2e": "ng e2e",
    "package": "npm run build:lib && (for %I in (LICENSE README.md CHANGELOG.md) do copy .\\%I .\\dist\\pip-webui2-themes) && cd dist/pip-webui2-themes && npm pack",
    "deploy": "ng build --prod --base-href https://pip-webui2.github.io/pip-webui2-themes/ && ngh --dir dist/pip-webui2-themes-app"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^9.0.2",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/router": "^9.0.2",
    "core-js": "^2.5.7",
    "node-sass": "^4.10.0",
    "rxjs": "~6.5.4",
    "scss-bundle": "^2.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular-devkit/build-ng-packagr": "~0.900.3",
    "@angular/cli": "^9.0.3",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "^9.0.2",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lodash": "^4.17.11",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5"
  }
}
