{
  "name": "pip-webui2-themes",
  "version": "1.2.0-rc.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "pip-webui2-themes build && ng serve",
    "build": "pip-webui2-themes build && ng build",
    "build:lib": "ng build pip-webui2-themes --prod && npm run build:lib-assets && npm run build:lib-cli && npm run build:lib-md",
    "build:lib-assets": "cpx \"projects/pip-webui2-themes/src/assets/**\" dist/pip-webui2-themes/assets",
    "build:lib-cli": "cpx \"projects/pip-webui2-themes/bin/**\" dist/pip-webui2-themes/bin && cpx \"projects/pip-webui2-themes/themes.schema.json\" dist/pip-webui2-themes",
    "build:lib-md": "cpx \"{LICENSE,README.md,CHANGELOG.md}\" dist/pip-webui2-themes",
    "test": "ng test",
    "test:lib": "ng test pip-webui2-themes",
    "lint": "ng lint",
    "lint:lib": "ng lint pip-webui2-themes",
    "e2e": "ng e2e",
    "package": "npm run build:lib && cd dist/pip-webui2-themes && npm pack",
    "deploy": "ng build --prod --base-href https://pip-webui2.github.io/pip-webui2-themes/ && ngh --dir dist/pip-webui2-themes-app"
  },
  "bin": {
    "pip-webui2-themes": "./projects/pip-webui2-themes/bin/cli.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.2",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/router": "^9.0.2",
    "core-js": "^3.6.4",
    "cpx": "^1.5.0",
    "esm": "^3.2.25",
    "lodash": "^4.17.11",
    "node-sass": "^4.13.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "yargs": "^15.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular-devkit/build-ng-packagr": "~0.900.3",
    "@angular/cli": "^9.0.3",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "^9.0.2",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5"
  }
}
